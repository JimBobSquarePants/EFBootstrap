  <connectionStrings>
    <add name="DbContextAdapter" connectionString="metadata=res://*/Chinook.csdl|
res://*/Chinook.ssdl|res://*/Chinook.msl;provider=System.Data.SqlServerCe.4.0;
provider connection string=&quot;Data Source=Chinook.sdf;
Persist Security Info=True&quot;" providerName="System.Data.EntityClient"/>
